	/* Title Style */
	.nine h1 {
		text-align: center;
		font-size: 50px;
		text-transform: uppercase;
		color: #222;
		letter-spacing: 1px;
		font-family: "Playfair Display", serif;
		font-weight: 400;
	}

		.nine h1 span {
